<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:id="@+id/img_banner"
        android:layout_width="0dp"
        android:layout_height="160dp"
        android:background="@color/light_transparent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="0dp" />


    <View
        android:id="@+id/img_knowledge"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:background="@color/light_transparent"
        app:layout_constraintLeft_toLeftOf="@+id/img_banner"
        app:layout_constraintRight_toLeftOf="@+id/img_story"
        app:layout_constraintTop_toBottomOf="@+id/img_banner" />

    <View
        android:id="@+id/tv_knowledge"
        android:layout_width="56dp"
        android:layout_height="10dp"
        android:layout_marginTop="10dp"
        android:background="@color/light_transparent"
        app:layout_constraintLeft_toLeftOf="@+id/img_knowledge"
        app:layout_constraintRight_toRightOf="@+id/img_knowledge"
        app:layout_constraintTop_toBottomOf="@+id/img_knowledge" />

    <View
        android:id="@+id/img_story"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:background="@color/light_transparent"
        app:layout_constraintLeft_toRightOf="@+id/img_knowledge"
        app:layout_constraintRight_toLeftOf="@+id/img_service"
        app:layout_constraintTop_toBottomOf="@+id/img_banner" />

    <View
        android:id="@+id/tv_story"
        android:layout_width="56dp"
        android:layout_height="10dp"
        android:layout_marginTop="10dp"
        android:background="@color/light_transparent"
        app:layout_constraintLeft_toLeftOf="@+id/img_story"
        app:layout_constraintRight_toRightOf="@+id/img_story"
        app:layout_constraintTop_toBottomOf="@+id/img_story" />

    <View
        android:id="@+id/img_service"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:background="@color/light_transparent"
        app:layout_constraintLeft_toRightOf="@+id/img_story"
        app:layout_constraintRight_toRightOf="@+id/img_banner"
        app:layout_constraintTop_toBottomOf="@+id/img_banner" />

    <View
        android:id="@+id/tv_service"
        android:layout_width="56dp"
        android:layout_height="10dp"
        android:layout_marginTop="10dp"
        android:background="@color/light_transparent"
        app:layout_constraintLeft_toLeftOf="@+id/img_service"
        app:layout_constraintRight_toRightOf="@+id/img_service"
        app:layout_constraintTop_toBottomOf="@+id/img_service" />

    <View
        android:id="@+id/divider_1"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="10dp"
        android:background="@color/colorLine"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintLeft_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_service"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="0dp" />

    <View
        android:id="@+id/img_center"
        android:layout_width="56dp"
        android:layout_height="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:background="@color/light_transparent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider_1" />

    <View
        android:layout_width="56dp"
        android:layout_height="20dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:background="@color/light_transparent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider_1" />


    <View
        android:id="@+id/img_bottom1"
        android:layout_width="56dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:background="@color/light_transparent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/tv_bottom1"
        app:layout_constraintTop_toBottomOf="@+id/img_center" />

    <View
        android:id="@+id/tv_bottom1"
        android:layout_width="56dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:background="@color/light_transparent"
        app:layout_constraintLeft_toRightOf="@+id/img_bottom1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_center" />
</android.support.constraint.ConstraintLayout>
