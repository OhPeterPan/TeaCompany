<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="timeTextView">
        <attr name="text" format="string" />
        <attr name="duration" format="integer" />
    </declare-styleable>
    <declare-styleable name="editFocusState">
        <attr name="normal_background" format="reference" />
        <attr name="focus_background" format="reference" />
        <attr name="is_integer" format="boolean" />
        <attr name="text_left" format="string" />
        <attr name="text_hint_more" format="string" />
    </declare-styleable>
</resources>